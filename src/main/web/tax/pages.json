	//一级导航栏
	"tabBar": {
		//tab上的文字默认值颜色
		"color": "#888888",
		//tab上文字选中时的颜色
		"selectedColor": "#007AFF",
		//tabbar 上边框的颜色，可选值 black/white
		"borderStyle": "black",
		//tab 的背景色	
		"backgroundColor": "#ffffff",
		// list 接收一个数组，数组中的每个项都是一个对象
		"list": [{
				//页面路径，必须在 pages 中先定义
				"pagePath": "pages/index/index",
				//tab 上按钮文字
				"text": "%index.home%",
				//未被选择时图片路径
				"iconPath": "static/images/tabbar-home.png",
				//选择后图片路径
				"selectedIconPath": "static/images/tabbar-home-selected.png"
			},{
				//myTeam页面，必须先在pages中先定义
				"pagePath": "pages/myTeam/index",
				//tab上按钮文字
				"text": "%index.myTeam%",
				"iconPath": "static/images/tabbar-myTeam.png",
				"selectedIconPath": "static/images/tabbar-myTeam-selected.png"

			},{
				//taxRefund页面，必须先在pages中先定义
				"pagePath": "pages/taxRefund/index",
				"text": "%index.taxRefund%",
				"iconPath": "static/images/tabbar-taxRefund.png",
				"selectedIconPath": "static/images/tabbar-taxRefund-selected.png"
			},
